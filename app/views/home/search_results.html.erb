
<div class="row-fluid">
  <div class="span8">
    <% if is_message_needed %>
      <div class="alert alert-success">
      <a href="#" class="close" data-dismiss="alert">&times;</a>
      <strong>Congrats!!!! </strong> You have successfully completed the challenge. Your score is <%=calculate_score %>.
    </div>
    <% end %>
      <div class="span12">
      <div class="search-box">
      <%= form_tag search_results_path, :method => :get, :remote => false do %>
      <div class="input-append">
        <div class="bing_image">
        <%= image_tag("bing.png", :alt => "home", :title=>"Bing Search") %>
      </div>
        <%= text_field_tag :search, params[:search], placeholder: "Enter Key word ", class:"input-xxlarge", :autocomplete => 'off'%>
      <%= button_tag( :class => "search-button") do %>
          <%= image_tag("search.png")%>
        <% end %>
    </div>
      <% end %>
    </div>
    <div id="search_results_box">
      <%= render :partial => 'search_results', :locals => { :search_results => @search_results } %>

  </div>

  </div>
  </div>


